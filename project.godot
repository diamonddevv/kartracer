config_version=5

[application]

config/name="KartRacer"
run/main_scene="res://scenes/screen/main_menu.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://assets/icon.svg"

[autoload]

DebugOverlay="*res://scenes/scripts/debug_overlay.tscn"
NetworkManager="*res://scripts/Network/NetworkManager.cs"
MenuManager="*res://scenes/screen/menu_manager.tscn"
GlobalManager="*res://scenes/scripts/global_manager.tscn"
Netcode="*res://scripts/Network/Netcode.cs"

[dotnet]

project/assembly_name="KartRacer"

[file_customization]

folder_colors={
"res://assets/": "red",
"res://prefabs/": "orange",
"res://scenes/": "yellow",
"res://scripts/": "green"
}

